<launch>

  <!-- base launch file -->
  <include file="$(ada_launch)/launch/base.launch">

  <!-- load ada urdf -->
  <param name="robot_description"
        command="cat $(find ada_description)/robots_urdf/ada_with_camera_forque.urdf"/>

  <!-- static_transform_publisher x y z yaw pitch roll frame_id child_frame_id period_in_ms -->
  <!-- multi view calibration -->
  <node name="st_joule2camera" pkg="tf" type="static_transform_publisher" respawn="false" output="screen"
        args="0.0986766 -0.00180981 0.020396 0.0763978 0.0133071 -1.56798 j2n6s200_joule camera_link 10"/>

  <include file="$(find depth_image_proc)/launch/cloudify.launch" />

  <node pkg="deep_pose_estimators" type="run_detection.sh" name="deep_pose_estimators_detection" output="screen" />

</launch>
